pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        echo 'Building..'
        sh "cd superherosfightingapp && make install"
        sh "cd superherosfightingapp && make build"
      }
    }
    stage('Test') {
      steps {
        echo 'Testing..'
      }
    }
    stage('Running') {
      steps {
        echo 'Deploying....'
        sh "cd superherosfightingapp && make run"
      }
    }
    
  }
}
