pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        echo 'Building..'
        sh "make install"
        sh "make build"
      }
    }
    stage('Test') {
      steps {
        echo 'Testing..'
      }
    }
    stage('Running') {
      steps {
        echo 'Deploying....'
        sh "make run"
      }
    }
  }
}
